{
  "kind": "collectionType",
  "collectionName": "webinars",
  "info": {
    "singularName": "webinar",
    "pluralName": "webinars",
    "displayName": "Webinar",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Sections": {
      "type": "dynamiczone",
      "components": [
        "layout.spotify-podcast-embed",
        "layout.video-embed",
        "layout.podcast-details",
        "layout.video-details"
      ]
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "Podcast",
        "Video"
      ],
      "required": true
    },
    "Thumbnail": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
